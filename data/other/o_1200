-erlich\/4005-12499\/"},{"aliases":null,"api_detail_url":"http:\/\/comicvine.gamespot.com\/api\/character\/4005-12500\/","birth":null,"count_of_issue_appearances":1,"date_added":"2008-06-06 11:26:42","date_last_updated":"2011-05-05 16:52:15","deck":null,"description":null,"first_appeared_in_issue":{"api_detail_url":"http:\/\/comicvine.gamespot.com\/api\/first_appeared_in_issue\/4000-74675\/","id":74675,"name":"The Rebirth of Princess Koriand'r","issue_number":"110"},"gender":0,"id":12500,"image":null,"name":"Teraizer","origin":null,"publisher":{"api_detail_url":"http:\/\/comicvine.gamespot.com\/api\/publisher\/4010-10\/","id":10,"name":"DC Comics"},"real_name":null,"site_detail_url":"http:\/\/comicvine.gamespot.com\/teraizer\/4005-12500\/"},{"aliases":null,"api_detail_url":"http:\/\/comicvine.gamespot.com\/api\/character\/4005-12509\/","birth":null,"count_of_issue_appearances":6,"date_added":"2008-06-06 11:26:53","date_last_updated":"2011-05-05 16:42:44","deck":null,"description":null,"first_appeared_in_issue":{"api_detail_url":"http:\/\/comicvine.gamespot.com\/api\/first_appeared_in_issue\/4000-37560\/","id":37560,"name":"The Darkening Part 3: The Brotherhood","issue_number":"99"},"gender":0,"id":12509,"image":{"icon_url":"http:\/\/comicvine.gamespot.com\/api\/image\/square_avatar\/878997-technis.jpg","medium_url":"http:\/\/comicvine.gamespot.com\/api\/image\/scale_medium\/878997-technis.jpg","screen_url":"http:\/\/comicvine.gamespot.com\/api\/image\/screen_medium\/878997-technis.jpg","small_url":"http:\/\/comicvine.gamespot.com\/api\/image\/scale_small\/878997-technis.jpg","super_url":"http:\/\/comicvine.gamespot.com\/api\/image\/scale_large\/878997-technis.jpg","thumb_url":"http:\/\/comicvine.gamespot.com\/api\/image\/scale_avatar\/878997-technis.jpg","tiny_url":"http:\/\/comicvine.gamespot.com\/api\/image\/square_mini\/878997-technis.jpg"},"name":"Technis","origin":null,"publisher":{"api_detail_url":"http:\/\/comicvine.gamespot.com\/api\/publisher\/4010-10\/","i